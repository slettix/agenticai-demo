    "governance_patterns": {
      "centralized_governance": {
        "description": "Single point of control with strong central authority",
        "when_to_use": "High complexity, critical services, strong retained capability",
        "pros": ["Clear accountability", "Consistent standards", "Effective coordination"],
        "cons": ["Potential bottleneck", "May slow decision-making", "Requires strong central team"]
      },
      "federated_governance": {
        "description": "Distributed decision-making with coordination mechanisms",
        "when_to_use": "Lower complexity, specialized domains, mature vendor relationships",
        "pros": ["Faster decisions", "Domain expertise", "Vendor empowerment"],
        "cons": ["Potential inconsistency", "Coordination challenges", "Accountability gaps"]
      },
      "hybrid_governance": {
        "description": "Mix of centralized and federated approaches by domain or decision type",
        "when_to_use": "Most real-world scenarios with varying complexity and criticality",
        "pros": ["Balanced approach", "Flexibility", "Appropriate control"],
        "cons": ["Complexity to design", "Clear boundaries needed", "Regular review required"]
      }
    },
    "contract_models": {
      "prime_contractor": {
        "description": "Single prime contractor with subcontractor relationships",
        "siam_role": "Integrated into prime contractor responsibilities",
        "risk_profile": "Lower client risk, higher dependency on prime"
      },
      "multi_prime": {
        "description": "Multiple direct contracts with client organization",
        "siam_role": "Client-retained or separate SIAM provider",
        "risk_profile": "Higher coordination complexity, more client control"
      },
      "partnership_model": {
        "description": "Formal partnership or joint venture between providers",
        "siam_role": "Shared SIAM function across partnership",
        "risk_profile": "Aligned incentives, complex governance requirements"
      }
    }
